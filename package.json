  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/creamdog/ngine/issues"
  },
  "homepage": "https://github.com/creamdog/ngine#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "uglify-js": "3.2.0",
    "http-server": "0.11.1",
    "babel-preset-env": "^1.7.0"
  },
  "dependencies": {
    "@babel/preset-es2015": "^7.0.0-beta.53"
  }
}
