{
  "name": "ngine",
  "version": "1.0.0",
  "description": "just templating",
  "main": "app.js",
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "start": "http-server .",
    "test": "echo \"Error: no test specified\" && exit 1",
